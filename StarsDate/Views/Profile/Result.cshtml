@model IEnumerable<StarsDate.Models.ProfileSearchResultVM>
@{
    ViewData["Title"] = "Result";
}

<h2>Result</h2>

@foreach (var item in Model)
{

    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">@Html.DisplayFor(model => item.DisplayName) - @Html.DisplayFor(model => item.Age)</h3>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-8">
                    @Html.DisplayFor(model => item.Description)
                </div>
                <div class="col-md-4">
                    <img src="~/uploads/@item.ProfilePicture" class="profilePicture" />

                </div>
            </div>
        </div>
    </div>
}


